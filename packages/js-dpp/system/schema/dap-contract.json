{
  "$schema": "https://schema.dash.org/platform-4-0-0/dash-schema/schema",
  "$id": "https://schema.dash.org/platform-4-0-0/system/dap-contract",
  "type": "object",
  "allOf": [
    {
      "$ref": "https://schema.dash.org/platform-4-0-0/dash-schema/schema"
    }
  ],
  "properties": {
    "name": {
      "type": "string",
      "minLength": 3,
      "maxLength": 24,
      "pattern": "^[a-zA-Z0-9-_]{1,255}$"
    },
    "version": {
      "type": "number",
      "multipleOf": 1.0,
      "minimum": 1
    },
    "objects": {
      "type": "object",
      "patternProperties": {
        "^[a-zA-Z0-9-_]{1,255}$": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "object"
            },
            "properties": {
              "type": "object",
              "propertyNames": {
                "pattern": "^[a-zA-Z0-9-_]{1,255}$"
              },
              "minProperties": 1,
              "maxProperties": 100
            },
            "additionalProperties": {
              "type": "boolean",
              "const": false
            }
          },
          "required": [
            "properties",
            "additionalProperties"
          ]
        }
      }
    }
  },
  "required": [
    "name",
    "version",
    "objects"
  ],
  "additionalItems": false
}
