{
  "$schema": "https://schema.dash.org/platform-4-0-0/dash-schema/schema",
  "$id": "https://schema.dash.org/platform-4-0-0/system/st-packet",
  "properties": {
    "contractId": {
      "type": "string",
      "minLength": 64,
      "maxLength": 64
    },
    "itemsMerkleRoot": {
      "type": "string",
      "minLength": 64,
      "maxLength": 64
    },
    "itemsHash": {
      "type": "string",
      "minLength": 64,
      "maxLength": 64
    },
    "objects": {
      "type": "array"
    },
    "contracts": {
      "type": "array"
    }
  },
  "oneOf": [
    {
      "properties": {
        "objects": {
          "maxItems": 0
        },
        "contracts": {
          "maxItems": 1
        }
      }
    },
    {
      "properties": {
        "objects": {
          "maxItems": 1000
        },
        "contracts": {
          "maxItems": 0
        }
      }
    }
  ],
  "required": [
    "contractId",
    "itemsMerkleRoot",
    "itemsHash",
    "objects",
    "contracts"
  ],
  "additionalProperties": false
}
