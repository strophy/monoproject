{
  "$schema": "https://schema.dash.org/platform-4-0-0/dash-schema/schema",
  "$id": "https://schema.dash.org/platform-4-0-0/system/st-packet",
  "properties": {
    "dapContractId": {
      "type": "string",
      "minLength": 64,
      "maxLength": 64
    },
    "itemsMerkleRoot": {
      "type": "string",
      "minLength": 64,
      "maxLength": 64
    },
    "itemsHash": {
      "type": "string",
      "minLength": 64,
      "maxLength": 64
    },
    "dapObjects": {
      "type": "array"
    },
    "dapContracts": {
      "type": "array"
    }
  },
  "oneOf": [
    {
      "properties": {
        "dapObjects": {
          "maxItems": 0
        },
        "dapContracts": {
          "maxItems": 1
        }
      }
    },
    {
      "properties": {
        "dapObjects": {
          "maxItems": 1000
        },
        "dapContracts": {
          "maxItems": 0
        }
      }
    }
  ],
  "required": [
    "dapContractId",
    "itemsMerkleRoot",
    "itemsHash",
    "dapObjects",
    "dapContracts"
  ],
  "additionalProperties": false
}
