{
  "$schema": "https://schema.dash.org/platform-4-0-0/dash-schema/schema",
  "$id": "https://schema.dash.org/platform-4-0-0/system/base/dap-object",
  "type": "object",
  "properties": {
    "$type": {
      "type": "string"
    },
    "$action": {
      "type": "number",
      "enum": [
        0,
        1,
        2
      ]
    },
    "$rev": {
      "type": "number",
      "multipleOf": 1.0,
      "minimum": 0
    }
  },
  "required": [
    "$type",
    "$rev",
    "$action"
  ]
}
