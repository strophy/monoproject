    "connect": "^3.6.6",
    "dotenv-expand": "^4.2.0",
    "dotenv-safe": "^6.1.0",
    "emittery": "^0.4.1",
    "ipfs-http-client": "^28.1.0",
    "jayson": "^2.1.1",
